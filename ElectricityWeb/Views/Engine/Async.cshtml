@model ElectricityWeb.Models.Engine.AsyncEngineModel

@{
    ViewBag.Title = "Асинхронный двигатель";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Scripts/Highcharts-4.0.1/js/highcharts.js"></script>

@*
    <div id="async-chart">
